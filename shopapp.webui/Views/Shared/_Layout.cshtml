<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>ShopApp</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel="stylesheet" href="/modules/bootstrap/dist/css/bootstrap.min.css">
    @RenderSection("CssDataTable",false)
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/css/style.css">
    @if(TempData["sliderInfo"]!=null){
        <link rel="stylesheet" href="~/css/slider.css">
        
    }
    
</head>
<body>
    @await Html.PartialAsync("_navbar")
    <div class="row mt-3">
        <div class="col-md-12">
        @if(TempData["sliderInfo"]!=null){
            <partial name="_Slider" model="@(TempData.Get<SliderModel>("sliderInfo"))"/>
        }
        
        </div>
    </div>
    <main class="mb-5 mt-5">
        <div class="container">
            @if(TempData["message"]!=null)
            {
                <partial name="_ResultMessage" model="@(TempData.Get<AlertMessage>("message"))"/>
            }
     
            @RenderBody()
        </div>
    </main>

    <footer class="py-5 bg-dark text-white text-center" >
        ShopApp | 2022
    </footer>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" ></script>
    @RenderSection("Scripts",false)


    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    @if(TempData["sliderInfo"]!=null){

        <script src="~/js/slider.js"></script>
    }
</body>
</html>